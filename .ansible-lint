  - ./molecule/release-static/roles/geerlingguy.postgresql
  - ./molecule/release-dynamic/roles/geerlingguy.postgresql
  - ./molecule/release-upgrade/roles/geerlingguy.postgresql
  - ./molecule/source-static/roles/geerlingguy.postgresql
  - ./molecule/source-dynamic/roles/geerlingguy.postgresql
  - ./molecule/source-upgrade/roles/geerlingguy.postgresql
